create sequence hibernate_sequence start with 1 increment by 1;
create table restaurant (id bigint not null, city varchar(255), dairy_score varchar(255), egg_score varchar(255), line1 varchar(255), name varchar(255), overall_score varchar(255), peanut_score varchar(255), phone_number varchar(255), state varchar(255), website varchar(255), zip_code varchar(255), primary key (id));
create table review (id bigint not null, dairy_score integer, egg_score integer, peanut_score integer, restaurant_id bigint, review varchar(255), status integer, submitted_by varchar(255), primary key (id));
create table user (id bigint not null, city varchar(255), dairy_watch boolean, display_name varchar(255), egg_watch boolean, peanut_watch boolean, state varchar(255), zip_code varchar(255), primary key (id));
